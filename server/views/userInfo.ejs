<%- include('header')%>
    <table class="table">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
          </tr>
        </thead>
        <tbody>
            <% userInfo.forEach((user, i) => { %>
                <tr>
                    <th scope="row"><%= user.id %></th>
                    <td><%= user.firstname %> <%= user.lastname %> </td>
                    <td><%= user.email%></td>
                  </tr>
            <% }) %>  
        </tbody>
      </table>
<%- include('footer')%>

